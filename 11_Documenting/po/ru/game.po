#: src/game.c:18
msgid "yes"
msgstr "да"

#: src/game.c:20
msgid "no"
msgstr "нет"

#: src/game.c:23
#, c-format
msgid "Invalid answer, please enter yes or no\n"
msgstr "Неверный ответ, пожалуйста, введите да или нет\n"

#: src/game.c:31
#, c-format
msgid "Let's play a game!\n"
msgstr "Давайте сыграем в игру!\n"

#: src/game.c:33
#, c-format
msgid "Think of a number between %s and %s\n"
msgstr "Загадайте число между %s и %s\n"

#: src/game.c:36
#, c-format
msgid "Think of a number between 1 and 100\n"
msgstr "Загадайте число между 1 и 100\n"

#: src/game.c:45
#, c-format
msgid "Is your number bigger than %s? (yes/no)\n"
msgstr "Ваше число больше %s? (да/нет)\n"

#: src/game.c:48
#, c-format
msgid "Is your number bigger than %d? (yes/no)\n"
msgstr "Ваше число больше %d? (да/нет)\n"

#: src/game.c:64
#, c-format
msgid "Your number is %s!\n"
msgstr "Ваше число %s!\n"

#: src/game.c:66
#, c-format
msgid "Your number is %d!\n"
msgstr "Ваше число %d!\n"

#: src/game.c:69
#, c-format
msgid "Do you want to play again? (yes/no)\n"
msgstr "Хотите сыграть еще раз? (да/нет)\n"

#: src/game.c:80
#, c-format
msgid "Usage: game [OPTION...]\n"
msgstr "Использование: game [ОПЦИЯ...]\n"

#: src/game.c:81
#, c-format
msgid "Guess a number between 1 and 100\n"
msgstr "Угадайте число между 1 и 100\n"

#: src/game.c:82
#, c-format
msgid "-r, --roman                Use Roman numeral system\n"
msgstr "-r, --roman                Использовать римскую систему счисления\n"

#: src/game.c:83
#, c-format
msgid "-h, --help                 Give this help list\n"
msgstr "-h, --help                 Вывести данное сообщение\n"

#: src/game.c:97
#, c-format
msgid "Invalid option: %s\n"
msgstr "Неверная опция: %s\n"

#: src/game.c:118
#, c-format
msgid "Roman numeral system is enabled\n"
msgstr "Используется римская система счисления\n"

#: src/game.c:120
#, c-format
msgid "Decimal system is enabled\n"
msgstr "Используется десятичная система счисления\n"
